func (api *API) <%= @klass.name %>_<%= @name %>() (*[]<%= @klass.name %>, error) {
  res, status, err := api.request("<%= endpoint %>", "<%= method %>", nil)

  if err != nil {
    return nil, err
  }

  if status != 200 {
    return nil, fmt.Errorf("Status returned: %d", status)
  }

  r := &map[string][]<%= @klass.name %>{}
  err = json.NewDecoder(res).Decode(r)

  fmt.Printf("things are: %v\n\n", *r)

  result := (*r)["products"]

	if err != nil {
		return nil, err
  }

  return &result, nil
}
